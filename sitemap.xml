<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://yorkstudenthomes.com/</loc>
        <changefreq>weekly</changefreq>
        <priority>1</priority>
    </url>

    <url>
        <loc>https://yorkstudenthomes.com/homes/</loc>
        <changefreq>monthly</changefreq>
        <priority>0.5</priority>
    </url>

    {% for home in site.homes %}
    <url>
        <loc>{{ site.url }}{{ home.url }}</loc>
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
    </url>
    {%- endfor %}

    <url>
        <loc>https://yorkstudenthomes.com/contact/</loc>
        <changefreq>yearly</changefreq>
        <priority>0.5</priority>
    </url>

    <url>
        <loc>https://yorkstudenthomes.com/maps/</loc>
        <changefreq>yearly</changefreq>
        <priority>0.7</priority>
    </url>
</urlset>
